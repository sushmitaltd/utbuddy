{
	"name": "boilerPlateForTS",
	"displayName": "boilerplate",
	"description": "generates boiler plate using jest framework",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:boilerPlateForTS.create_unit_test_boiler_plate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "boilerPlateForTS.create_unit_test_boiler_plate",
				"title": "Create Unit test boiler plate"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceExtname==.ts",
					"command": "boilerPlateForTS.create_unit_test_boiler_plate"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"helmet": "^4.6.0",
		"html-to-mrkdwn": "^3.0.0",
		"my-precious": "^2.0.0"
	}
}
